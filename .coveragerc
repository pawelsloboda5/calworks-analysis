[run]
source=Script_python
omit=
    */tests/*
    */site-packages/*
    setup.py
    conftest.py

[report]
exclude_lines=
    pragma: no cover
    def __repr__
    if self\.debug
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError

[html]
directory=coverage_html_report
